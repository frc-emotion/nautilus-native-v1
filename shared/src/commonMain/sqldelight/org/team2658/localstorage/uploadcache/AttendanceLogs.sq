CREATE TABLE AttendanceLogsUploadCache (
    meeting_id TEXT UNIQUE NOT NULL PRIMARY KEY,
    tap_time INTEGER NOT NULL,
    verifiedBy TEXT NOT NULL
);

insert:
INSERT INTO AttendanceLogsUploadCache (meeting_id, tap_time, verifiedBy) VALUES (?, ?, ?);

delete:
DELETE FROM AttendanceLogsUploadCache WHERE meeting_id = ?;

clear:
DELETE FROM AttendanceLogsUploadCache;

get:
SELECT * FROM AttendanceLogsUploadCache;
